<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <configSections>
    <section name="paralib" type="com.paralib.Configuration.ParalibSection, paralib-common, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
  </configSections>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>

  <connectionStrings>
    <add name="paralib" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=oovent;Integrated Security=True;" />
  </connectionStrings>

  <paralib connection="paralib" >

    <logging>
      <logs>
        <log name="logger0" type="Console" capture="error,fatal,info-warn" pattern="%thread [%property{tid}] %level %c %property{method} %property{line} %property{user} %n"/>
        <log name="logger1" type="Database" pattern="%method" table="joe" fields="t" />
      </logs>
    </logging>
  
  </paralib>



</configuration>